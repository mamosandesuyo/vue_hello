<div class="container" data-js-action='message-room-index' id='app' data-message-rooms='<%= @message_rooms.to_json %>'>
  <h1>メッセージ</h1>

  <table class="table table-striped table-bordered">
    <tr v-for='room in messageRooms'>
      <td>{{ room.title }}</td>
      <td>
        <a :href="'/message_rooms/' + room.id " class="btn btn-primary">見る</a>
      </td>
    </tr>
  </table>

  <form action="#">
    <div class="form-group">
      <label for="">タイトル</label>
      <input type="text" class="form-control" v-model='newRoom.title'>
    </div>
    <div class="form-group">
      <a href="#" class="btn btn-primary" @click.prevent='submit'>投稿</a>
    </div>
  </form>
</div>